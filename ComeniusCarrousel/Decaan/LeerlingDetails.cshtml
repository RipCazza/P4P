@{
    Page.Title= "Keuze";
    Layout = "~/_Layout.cshtml";

    var leerlingId;

    if (!User.IsInRole("Admin") && !User.IsInRole("Decaan"))
    {
        Response.Redirect("~/");
    }

    if(!Request.QueryString["ID"].IsEmpty() && Request.QueryString["ID"].IsInt()) 
    {
        leerlingId = Request.QueryString["ID"];
        LeerlingRepo.Leerling leerling = LeerlingRepo.GetLeerling(leerlingId);
    }
}
