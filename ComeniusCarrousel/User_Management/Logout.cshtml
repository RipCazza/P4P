@*
    Permissies: ~
    Wordt aangeroepen in: _Layout
    Linkt door naar: Default
    Beschrijving: Het uitloggen van een gebruiker
*@
@{
    if(!Request.QueryString["ReturnUrl"].IsEmpty())
    {

        WebSecurity.Logout();

        var returnUrl = Request.QueryString["ReturnUrl"];
        Context.RedirectLocal(returnUrl);
    }
    else{
        Response.Redirect("~/");
    }
}